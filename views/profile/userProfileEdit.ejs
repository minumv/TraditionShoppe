<%- include('../layout/profileHeader') %>
<div class="overview d-block">
 
        <form action="/changeprofile/<%= usr._id%>" method="post"></form> 
        <div class="edit-box">  
            <div class="edit-head"><p>Edit Details</p></div>      
            <div><input class="edit-text" type="text" name="name" value="<%= users.name %>"></div>
            <div><input class="edit-text" type="text" name="contact" value="<%= users.phone %>"></div>
            <div><input class="edit-text" type="text" name="mail" value="<%= users.email %>"></div>
            <div><button class="edit-but" type="submit">Save Details</button></div>
        </div>
        </form>    



</div>

  </div>

  <%- include('../layout/userFooter') %>